<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Planning\Bundle\Entity\Classe" table="classe">
    <id name="idclasse" type="integer" column="idclasse">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelle" type="string" column="libelle" length="45" nullable="true"/>
    <many-to-many field="promotionpromotion" target-entity="Promotion" mapped-by="classeclasse"/>
  </entity>
</doctrine-mapping>
