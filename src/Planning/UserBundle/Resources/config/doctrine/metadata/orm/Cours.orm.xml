<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cours" table="cours">
    <id name="idcours" type="integer" column="idcours">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dure" type="integer" column="dure" nullable="true"/>
    <field name="dateDebut" type="date" column="date_debut" nullable="true"/>
    <field name="dateFin" type="date" column="date_fin" nullable="true"/>
  </entity>
</doctrine-mapping>
